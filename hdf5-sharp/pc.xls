<?xml version="1.0" encoding="ISO-8859-1"?>

<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
<xsl:output method="text" />
<xsl:param name="prefix" />

<xsl:template match="/">
prefix=<xsl:value-of select="$prefix"/>
assemblies_dir=${prefix}/lib/<xsl:value-of select="project/name" />
Libraries=${assemblies_dir}/hdf5-sharp.dll

Name: <xsl:value-of select="project/name" />
Description: <xsl:value-of select="project/description" />
Version: <xsl:value-of select="project/version" />
Libs: -r:hdf5-sharp.dll
</xsl:template>

</xsl:stylesheet>
